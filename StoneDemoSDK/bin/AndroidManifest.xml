<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.stone.stonedemosdk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.com.stone.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.stone.activity.TransactionActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="br.com.stone.activity.CancellationActivity"
            android:label="@string/title_activity_cancellation" >
        </activity>
        <activity
            android:name="br.com.stone.activity.TypedSaleActivity"
            android:label="@string/title_activity_typed" >
        </activity>
    </application>

</manifest>